<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.malka.androidappp">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <supports-screens
        android:anyDensity="true"
        android:largeScreens="true"
        android:normalScreens="true"
        android:smallScreens="true"
        android:xlargeScreens="true" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/malqa_iconn"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/malqa_iconn_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:ignore="AllowBackup">
        <activity
            android:name=".design.SellerInformation"
            android:exported="false" />
        <activity
            android:name=".activities_main.order.SuccessOrder"
            android:exported="false" />
        <activity
            android:name=".activities_main.order.AddressPaymentActivity"
            android:exported="true"></activity>
        <activity
            android:name=".activities_main.product_detail.QuestionActivity"
            android:exported="false" />
        <activity
            android:name=".activities_main.place_picker.LocationPickerActivity"
            android:exported="false" />
        <activity
            android:name=".activities_main.business_signup.Switch_Account"
            android:exported="true" />
        <activity
            android:name=".activities_main.FullImageActivity"
            android:exported="false" />
        <activity
            android:name=".activities_main.PlayActivity"
            android:exported="false" />
        <activity
            android:name=".activities_main.order.CartActivity"
            android:exported="true" />
        <activity
            android:name=".design.order_done"
            android:exported="false" />
        <activity
            android:name=".design.negotiation_offers1"
            android:exported="true" />
        <activity
            android:name=".design.ProductReviews"
            android:exported="true" />
        <activity
            android:name=".activities_main.product_detail.ProductDetails"
            android:exported="true" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity android:name=".botmnav_fragments.activities_main.business_signup.BusinessAccountCreate" />
        <activity android:name=".activities_main.forgot.ForgotChangepassActivity" />
        <activity android:name=".activities_main.forgot.ActivityForgotPassOtpcode" />
        <activity
            android:name=".activities_main.SplashActivity"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities_main.MainActivity" />
        <activity
            android:name=".activities_main.login.SignInActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".activities_main.signup_account.signup_pg1.SignupPg1" />
        <activity android:name=".activities_main.signup_account.signup_pg2.SignupPg2" />
        <activity android:name=".activities_main.signup_account.signup_pg3.SignupPg3" />
        <activity android:name=".activities_main.forgot.ForgotPasswordActivty" />
        <activity android:name=".activities_main.add_product.ListanItem" />
        <activity android:name=".activities_main.add_product.ChooseCategory" />
        <activity android:name=".activities_main.add_product.all_categories.SubCategories" />
        <activity android:name=".activities_main.add_product.AddPhoto" />
        <activity android:name=".activities_main.add_product.DynamicTemplate" />
        <activity android:name=".activities_main.add_product.ListingDetails" />
        <activity android:name=".activities_main.add_product.PricingActivity" />
        <activity android:name=".activities_main.add_product.ListingDuration" />
        <activity android:name=".activities_main.add_product.PromotionalActivity" />
        <activity android:name=".activities_main.add_product.Confirmation" />
        <activity android:name=".activities_main.add_product.SuccessProduct" />
