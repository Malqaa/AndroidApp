            android:name="com.zfdang.multiple_images_selector.ImagesSelectorActivity"
            android:configChanges="orientation|screenSize"
            android:hardwareAccelerated="false" />
        <activity android:name=".botmnav_fragments.browse_market.BrowseMarketFragment" />
        <activity android:name=".botmnav_fragments.account_fragment.address.AddAddress" />
