<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginBottom="?actionBarSize"
    tools:context=".botmnav_fragments.unsold_business.UnsoldBusiness">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar_unsold_business"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:layout_alignParentTop="true"
        android:background="#339933"
        app:popupTheme="@style/PopupOverlay" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/unsold_business_recycler"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/toolbar_unsold_business" />
</RelativeLayout>