<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context=".fragments.memberlisting.MemberListing">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar_memberlisting"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:layout_alignParentTop="true"
        android:background="#339933"
        app:popupTheme="@style/PopupOverlay">

    </androidx.appcompat.widget.Toolbar>

    <include
        android:id="@+id/memberlising_card1"
        layout="@layout/card_memberlisting"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/toolbar_memberlisting"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true" />


    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerViewML"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/memberlising_card1" />


</RelativeLayout>