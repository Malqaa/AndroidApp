<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/appBarLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:theme="@style/AppBarOverlay">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar111"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="#339933"
            app:navigationIcon="@drawable/nav_icon_back"
            app:popupTheme="@style/PopupOverlay">
        </androidx.appcompat.widget.Toolbar>
    </com.google.android.material.appbar.AppBarLayout>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerView22"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@id/button261"
        android:layout_below="@id/appBarLayout"
        android:layout_marginBottom="2dp" />
    <Button
        android:id="@+id/button261"
        android:layout_width="match_parent"
        android:layout_height="64dp"
        android:layout_alignParentBottom="true"
        android:background="@color/GoldenApp"
        android:text="REFINE"
        android:textColor="#ffffff"
        android:textSize="22dp" />
</RelativeLayout>